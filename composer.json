{
    "name": "PascalKleindienst/FormListGenerator",
    "type": "library",
    "description": "",
    "keywords": [
        "PascalKleindienst",
        "FormListGenerator"
    ],
    "homepage": "https://github.com/PascalKleindienst/FormListGenerator",
    "license": "MIT",
    "authors": [
        {
            "name": "Pascal Kleindienst",
            "email": "mail@pascalkleindienst.de",
            "homepage": "https://github.com/PascalKleindienst",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.5|~5.6|~7.0",
        "symfony/yaml": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0||~5.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "PascalKleindienst\\FormListGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PascalKleindienst\\FormListGenerator\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "example-server": "php -S localhost:8888 -t examples examples/example.php"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}